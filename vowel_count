def vowels_count(str):
    count = 0
    for char in str:
        if char == 'a' or char == 'e' or char == 'i' or char == 'o' or char == 'u' or char == 'A' or char == 'E' or char == 'I' or char== 'O' or char == 'U':
            count += 1
    print(count)

str = input("Enter a string: ")
vowels_count(str)
